<?xml version="1.0" encoding="UTF-8"?>
<doc>
	<panel
	 	id="panel">
	 		
 		<panel
 			background="C7E1EA"
 			opaque="true"
 			layout="GridBagLayout">
			 		
	 		<gridbagconstraints
				gridx="0"
				gridy="0"
	 			weightx="1"
				insets="0,0,4,0"
				fill="GridBagConstraints.HORIZONTAL"
	 			/>
 			
		 	<label
		 		text="MIDI / OSC Settings">
		 		<gridbagconstraints
	 				weightx="1"
	 				insets="8,10,8,0"
					fill="GridBagConstraints.BOTH"
		 			/>
		 	</label>
		 	
	 		<button
		 		id="SETTINGS_BUTTON_EXPAND_MIDI_OSC"
		 		actioncommand="SETTINGS_BUTTON_EXPAND_MIDI_OSC"
		 		text="Close">
		 		<gridbagconstraints
		 			anchor="GridBagConstraints.LINE_END"
		 			/>
		 	</button>
	 	</panel>
	 	
	 	<panel
	 		id="SETTINGS_CONTENT_PANEL_MIDI_OSC"
			opaque="false"
 			layout="GridBagLayout">
	 		<gridbagconstraints
				gridx="0"
				gridy="1"
	 			weightx="1"
				insets="0,0,4,0"
				fill="GridBagConstraints.HORIZONTAL"
	 			/>
	 			
	 		<label
	 			text="Channel">
		 		<gridbagconstraints
					gridx="0"
					gridy="0"
	 				insets="0,10,0,0"
					anchor="GridBagConstraints.LINE_START"
		 			/>
	 		</label>
	 			
		 	<slider
		 		id="SETTINGS_MIDI_CHANNEL_SLIDER"
		 		minimum="1"
		 		maximum="16"
		 		orientation="SwingConstants.HORIZONTAL"
		 		preferredsize="100,29">
		 		<gridbagconstraints
					gridx="1"
					gridy="0"
	 				weightx="1"
					fill="GridBagConstraints.HORIZONTAL"
		 			/>
			</slider>
	 			
	 		<label
		 		id="SETTINGS_MIDI_CHANNEL_TEXT"
	 			text="0"
	 			preferredsize="40,24">
		 		<gridbagconstraints
					gridx="2"
					gridy="0"
					anchor="GridBagConstraints.LINE_END"
		 			/>
	 		</label>
	 			
	 		<label
	 			text="Pitch">
		 		<gridbagconstraints
					gridx="0"
					gridy="1"
	 				insets="0,10,0,0"
					anchor="GridBagConstraints.LINE_START"
		 			/>
	 		</label>
	 			
		 	<slider
		 		id="SETTINGS_MIDI_PITCH_SLIDER"
		 		minimum="0"
		 		maximum="127"
		 		orientation="SwingConstants.HORIZONTAL"
		 		preferredsize="100,29">
		 		<gridbagconstraints
					gridx="1"
					gridy="1"
	 				weightx="1"
					fill="GridBagConstraints.HORIZONTAL"
		 			/>
			</slider>
	 			
	 		<label
		 		id="SETTINGS_MIDI_PITCH_TEXT"
	 			text="0"
	 			preferredsize="40,24">
		 		<gridbagconstraints
					gridx="2"
					gridy="1"
					anchor="GridBagConstraints.LINE_END"
		 			/>
	 		</label>
	 			
	 		<label
	 			text="Velocity">
		 		<gridbagconstraints
					gridx="0"
					gridy="2"
	 				insets="0,10,0,0"
					anchor="GridBagConstraints.LINE_START"
		 			/>
	 		</label>
	 			
		 	<slider
		 		id="SETTINGS_MIDI_VELOCITY_SLIDER"
		 		minimum="0"
		 		maximum="127"
		 		orientation="SwingConstants.HORIZONTAL"
		 		preferredsize="100,29">
		 		<gridbagconstraints
					gridx="1"
					gridy="2"
	 				weightx="1"
					fill="GridBagConstraints.HORIZONTAL"
		 			/>
			</slider>
	 			
	 		<label
		 		id="SETTINGS_MIDI_VELOCITY_TEXT"
	 			text="0"
	 			preferredsize="40,24">
		 		<gridbagconstraints
					gridx="2"
					gridy="2"
					anchor="GridBagConstraints.LINE_END"
		 			/>
	 		</label>
	 		
	 		<panel
				opaque="false"
	 			layout="GridBagLayout">
		 		<gridbagconstraints
					gridx="0"
					gridy="3"
		 			gridwidth="3"
					fill="GridBagConstraints.HORIZONTAL"
		 			/>
	 			
		 		<label
		 			text="OSC Address">
			 		<gridbagconstraints
						gridx="0"
						gridy="0"
		 				insets="0,10,0,0"
						anchor="GridBagConstraints.LINE_START"
			 			/>
		 		</label>
		 			
			 	<textfield
			 		id="SETTINGS_OSC_ADDRESS">
			 		<gridbagconstraints
						gridx="1"
						gridy="0"
						weightx="1"
		 				insets="0,10,0,50"
						fill="GridBagConstraints.HORIZONTAL"
			 			/>
				</textfield>
			</panel>
		</panel>
 	</panel>
</doc>